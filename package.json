{
  "name": "blekingetrafiken",
  "version": "0.1.0",
  "description": "An API to retrieve public transport information, schedules and routes in Blekinge",
  "main": "index.js",
  "scripts": {
    "test": "xo && nyc ava -v test/*.js"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/alexgustafsson/blekingetrafiken.git"
  },
  "keywords": [
    "bus",
    "public",
    "transportation",
    "commute",
    "train",
    "blekinge",
    "api"
  ],
  "author": "Alex Gustafsson",
  "license": "ISC",
  "devDependencies": {
    "ava": "^0.19.1",
    "eslint-plugin-ava": "^4.2.2",
    "eslint-plugin-unicorn": "^2.1.2",
    "nyc": "^11.2.1",
    "xo": "^0.18.2"
  },
  "xo": {
    "space": 2,
    "esnext": true,
    "rules": {
      "curly": [
        "error",
        "multi-or-nest",
        "consistent"
      ],
      "no-use-before-define": [
        "error",
        {
          "classes": false
        }
      ],
      "unicorn/catch-error-name": [
        "error",
        {
          "name": "error"
        }
      ],
      "dot-notation": [
        "off"
      ],
      "quote-props": [
        "off"
      ],
      "object-curly-spacing": [
        "off"
      ]
    }
  },
  "dependencies": {
    "axios": "^0.16.2",
    "moment": "^2.21.0",
    "xml2js": "^0.4.19"
  }
}
